{
  "name": "componentsjs",
  "version": "6.0.1",
  "description": "A semantic dependency injection framework",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/componentsjs/^5.0.0/components/context.jsonld": "components/context.jsonld"
  },
  "main": "cjs/lib/index.js",
  "module": "esm/lib/index.js",
  "types": "esm/lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./esm/lib/index.js",
        "types": "./esm/lib/index.d.ts"
      },
      "require": {
        "default": "./cjs/lib/index.cjs",
        "types": "./cjs/lib/index.d.ts"
      }
    },
    "./lib/*": {
      "import": {
        "default": "./esm/lib/*.js",
        "types": "./esm/lib/*.d.ts"
      },
      "require": {
        "default": "./cjs/lib/*.cjs",
        "types": "./cjs/lib/*.d.ts"
      }
    }
  },
  "bin": {
    "componentsjs-compile-config": "./cjs/bin/compile-config.js"
  },
  "typings": "index",
  "repository": "https://github.com/LinkedSoftwareDependencies/Components.js",
  "keywords": [
    "lsd",
    "linked software dependencies",
    "components",
    "dependency injection"
  ],
  "author": "Ruben Taelman <ruben.taelman@ugent.be>",
  "license": "MIT",
  "packageManager": "yarn@4.0.1",
  "scripts": {
    "build:esm": "rm -rf ./esm && tsc",
    "build:cjs": "rm -rf ./cjs && babel lib --out-dir cjs/lib --extensions .ts --out-file-extension .cjs && babel bin --out-dir cjs/bin --extensions .ts --out-file-extension .cjs && tsc --emitDeclarationOnly --outDir cjs",
    "build:ts": "yarn run build:esm && yarn run build:cjs",
    "test": "yarn test:jest",
    "test:jest": "jest",
    "test:webpack": "yarn test:webpack:node && yarn test:webpack:web",
    "test:webpack:node": "webpack -c test/webpack/webpack.config.js && node test/webpack/build/test.min.js",
    "test:webpack:web": "webpack -c test/webpack/webpack.config-web.js && node test/webpack/build-web/test.min.js",
    "test-watch": "jest --watch",
    "build": "scopy ./components/context.jsonld ./components/context.json && yarn run build:ts && chmod +x ./esm/bin/compile-config.js",
    "build-watch": "tsc --watch",
    "build:tsc": "tsc",
    "lint": "eslint . --ext .ts --cache",
    "validate": "yarn info",
    "prepare": "yarn build",
    "version": "manual-git-changelog onversion"
  },
  "dependencies": {
    "@comunica/actor-abstract-mediatyped": "file:../../comunica/comunica/packages/actor-abstract-mediatyped",
    "@comunica/actor-abstract-path": "file:../../comunica/comunica/packages/actor-abstract-path",
    "@comunica/actor-http-fetch": "file:../../comunica/comunica/packages/actor-http-fetch",
