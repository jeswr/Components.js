    "@comunica/bus-http": "file:../../comunica/comunica/packages/bus-http",
    "@comunica/bus-init": "file:../../comunica/comunica/packages/bus-init",
    "@comunica/bus-rdf-parse": "file:../../comunica/comunica/packages/bus-rdf-parse",
    "@comunica/bus-rdf-parse-html": "file:../../comunica/comunica/packages/bus-rdf-parse-html",
